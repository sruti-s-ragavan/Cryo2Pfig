<?xml version="1.0"?>
<!-- Include substrings PFIS, __ (double underscore) for suffix and filenames with discretion. See runScript.py for details. -->
<options>
    <algorithms graphType="PfisGraph" suffix = "VariantUnaware">
		<algorithm class="PFIS" name="PFIS with history, spread2"
			enabled="true" fileName="pfis_history_spread2.txt" history="true" numSpread="2" >
		</algorithm>
	</algorithms>
	<algorithms graphType="PfisGraphWithVariants" suffix = "NoCollapse">
		<algorithm class="PFIS" name="PFIS with history, spread2"
			enabled="true" fileName="pfis_history_spread2.txt" history="true" numSpread="2" >
		</algorithm>
	</algorithms>
	<algorithms graphType="PfisGraphWithSimilarPatches" variantsDb="variations_textSimilarity.db" suffix="TextCollapse">
		<algorithm class="PFIS" name="PFIS with history, spread2"
			enabled="true" fileName="pfis_history_spread2.txt" history="true" numSpread="2" >
		</algorithm>
	</algorithms>
	<algorithms graphType="PfisGraphWithSimilarPatches" variantsDb="variations_topologyAndTextSimilarity.db" suffix = "PureEquivalenceCollapse">
		<algorithm class="PFIS" name="PFIS with history, spread2"
			enabled="true" fileName="pfis_history_spread2.txt" history="true" numSpread="2" >
		</algorithm>
	</algorithms>
	<algorithms graphType="PfisGraph" suffix = "VariantUnaware-LesserUnknowns" navPathType="VariantAware">
		<algorithm class="PFIS" name="PFIS with history, spread2"
			enabled="true" fileName="pfis_history_spread2.txt" history="true" numSpread="2" >
		</algorithm>
	</algorithms>
	<algorithms graphType="PfisGraphWithVariants" suffix = "NoCollapse-LesserUnknowns" navPathType="VariantAware">
		<algorithm class="PFIS" name="PFIS with history, spread2"
			enabled="true" fileName="pfis_history_spread2.txt" history="true" numSpread="2" >
		</algorithm>
	</algorithms>
	<algorithms graphType="PfisGraphWithSimilarPatches" variantsDb="variations_textSimilarity.db" suffix="TextCollapse-LesserUnknowns"
	navPathType="VariantAware">
		<algorithm class="PFIS" name="PFIS with history, spread2"
			enabled="true" fileName="pfis_history_spread2.txt" history="true" numSpread="2" >
		</algorithm>
	</algorithms>
	<algorithms graphType="PfisGraphWithSimilarPatches" variantsDb="variations_topologyAndTextSimilarity.db"
				suffix = "PureEquivalenceCollapse-LesserUnknowns" navPathType="VariantAware">
		<algorithm class="PFIS" name="PFIS with history, spread2"
			enabled="true" fileName="pfis_history_spread2.txt" history="true" numSpread="2" >
		</algorithm>
	</algorithms>
</options>


